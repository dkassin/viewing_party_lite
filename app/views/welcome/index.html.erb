

<h1>Viewing Party Lite</h1>
<% if session[:user_id] %>
  <%= link_to "Logout", '/logout', method: :delete %>
<% else %>
  <%= button_to "Create a New User", "/register", method: :get %>
  <%= link_to "Log In", "/login" %>
<% end %>

<h3> Current Users </h3>


<% @users.each do |user| %>
  <div id="user-<%= user.id %>">
    <p><%= link_to "#{user.email}", "/dashboard" %>'s Dashboard</p>
  </div>
<% end %>
